<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_checked="false">
        <shape android:shape="oval">
            <solid android:color="@color/darkOrange"/>
            <size android:width="@dimen/switchCustom"
                android:height="@dimen/switchCustom"/>
            <stroke android:width="1dp"
                android:color="@color/darkOrange"/>
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:shape="oval">
            <solid android:color="@color/darkGreenPistacho"/>
            <size android:width="@dimen/switchCustom"
                android:height="@dimen/switchCustom"/>
            <stroke android:width="1dp"
                android:color="@color/darkGreenPistacho"/>
        </shape>
    </item>
</selector>